<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nyankobox">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".TitleActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PassChangeActivity" android:windowSoftInputMode="stateAlwaysVisible" ></activity>
        <activity
            android:name=".PassLock2Activity"
            android:windowSoftInputMode="stateAlwaysVisible" />
        <activity
            android:name=".PassLockActivity"
            android:windowSoftInputMode="stateAlwaysVisible" />
        <activity android:name=".PrivacyActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".LogRefeActivity" />
        <activity android:name=".DressActivity" />
        <activity android:name=".LogActivity" />
        <activity android:name=".DiaryActivity" />
        <activity android:name=".MainActivity">
        </activity>
    </application>

</manifest>